<?xml version="1.0"?>
<grammar type="window" version="1.0" author="Gen Nishida">
	<attr name="offset_size" value="0.05" range="0.05,0.05"/>
	<attr name="window_sill_height" value="0.05" range="0.05,0.05"/>
	
	<rule name="Window">
		<split splitAxis="y">
			<param type="relative" value="window_sill_height" name="WindowSillTile_12"/>
			<param type="relative" value="1-window_sill_height" name="WindowTileMain_12"/>
		</split>
	</rule>
	<rule name="WindowSillTile_12">
		<split splitAxis="y">
			<param type="relative" value="0.5" name="WindowSillTileLayer1_12"/>
			<param type="relative" value="0.5" name="WindowSillTileLayer2_12"/>
		</split>
	</rule>
	<rule name="WindowSillTileLayer1_12">
		<split splitAxis="x">
			<param type="relative" value="0.05" name="Wall"/>
			<param type="floating" value="1" name="WindowSillTileLayer1Main_12"/>
			<param type="relative" value="0.05" name="Wall"/>
		</split>
	</rule>
	<rule name="WindowSillTileLayer1Main_12">
		<extrude height="0.1"/>
		<comp>
			<param name="top" value="WindowSill"/>
			<param name="side" value="WindowSill"/>
		</comp>
	</rule>
	<rule name="WindowSillTileLayer2_12">
		<extrude height="0.2"/>
		<comp>
			<param name="top" value="WindowSill"/>
			<param name="side" value="WindowSill"/>
		</comp>
	</rule>
	<rule name="WindowTileMain_12">
		<split splitAxis="x">
			<param type="relative" value="0.05" name="Wall"/>
			<param type="floating" value="1" name="WindowTileMainMain_12"/>
			<param type="relative" value="0.05" name="Wall"/>
		</split>
	</rule>
	<rule name="WindowTileMainMain_12">
		<extrude height="0.1"/>
		<comp>
			<param name="side" value="WindowFrame"/>
			<param name="top" value="WindowTileMainMainTop_12"/>
		</comp>
	</rule>
	<rule name="WindowTileMainMainTop_12">
		<offset offsetDistance="-offset_size" inside="WindowTileMainMainTopInside_12" border="WindowFrame"/>
	</rule>
	<rule name="WindowTileMainMainTopInside_12">
		<extrude height="-0.1"/>
		<comp>
			<param name="side" value="WindowFrame"/>
			<param name="top" value="WindowTileMainMainTopInsideBottom_12"/>
		</comp>
	</rule>
	<rule name="WindowTileMainMainTopInsideBottom_12">
		<offset offsetDistance="-0.02" inside="WindowTileMainMainTopInsideBottomInside_12" border="WindowFrame"/>
	</rule>
	<rule name="WindowTileMainMainTopInsideBottomInside_12">
		<extrude height="-0.05"/>
		<comp>
			<param name="side" value="WindowFrame"/>
			<param name="top" value="WindowGlassArea_12"/>
		</comp>
	</rule>
	<rule name="WindowGlassArea_12">
		<setupProjection axesSelector="scope.xy">
			<param name="texWidth" type="relative" value="1"/>
			<param name="texHeight" type="relative" value="1"/>
		</setupProjection>
		<split splitAxis="x">
			<param type="relative" value="0.5" name="WindowGlassColumnArea_12" repeat="true"/>
		</split>
	</rule>
	<rule name="WindowGlassColumnArea_12">
		<split splitAxis="y">
			<param type="relative" value="0.7" name="WindowGlassCellArea_12"/>
			<param type="relative" value="0.3" name="WindowGlassCellAreaBottom_12"/>
		</split>
	</rule>
	<rule name="WindowGlassCellAreaBottom_12">
		<split splitAxis="x">
			<param type="relative" value="0.5" name="WindowGlassCellArea_12"/>
			<param type="relative" value="0.5" name="WindowGlassCellArea_12"/>
		</split>
	</rule>
	<rule name="WindowGlassCellArea_12">
		<split splitAxis="x">
			<param type="relative" value="0.025" name="WindowGlassSeparateTile_12"/>
			<param type="floating" value="1" name="WindowGlassCellColumnArea_12"/>
			<param type="relative" value="0.025" name="WindowGlassSeparateTile_12"/>
		</split>
	</rule>
	<rule name="WindowGlassCellColumnArea_12">
		<split splitAxis="y">
			<param type="relative" value="0.025" name="WindowGlassSeparateTile_12"/>
			<param type="floating" value="1" name="WindowGlass"/>
			<param type="relative" value="0.025" name="WindowGlassSeparateTile_12"/>
		</split>
	</rule>
	<rule name="WindowGlassSeparateTile_12">
		<extrude height="0.05"/>
		<comp>
			<param name="side" value="WindowFrame"/>
			<param name="top" value="WindowFrame"/>
		</comp>
	</rule>
</grammar>
