<?xml version="1.0"?>
<grammar type="window" version="1.0" author="Gen Nishida">
	<attr name="offset_size" value="0.05" range="0.05,0.05"/>
	<attr name="window_sill_height" value="0.05" range="0.05,0.05"/>
	
	<rule name="Window">
		<split splitAxis="y">
			<param type="relative" value="window_sill_height" name="WindowSillTile_09"/>
			<param type="relative" value="1-window_sill_height" name="WindowTileMain_09"/>
		</split>
	</rule>
	<rule name="WindowSillTile_09">
		<split splitAxis="y">
			<param type="relative" value="0.5" name="WindowSillTileLayer1_09"/>
			<param type="relative" value="0.5" name="WindowSillTileLayer2_09"/>
		</split>
	</rule>
	<rule name="WindowSillTileLayer1_09">
		<split splitAxis="x">
			<param type="relative" value="0.05" name="Wall"/>
			<param type="floating" value="1" name="WindowSillTileLayer1Main_09"/>
			<param type="relative" value="0.05" name="Wall"/>
		</split>
	</rule>
	<rule name="WindowSillTileLayer1Main_09">
		<extrude height="0.1"/>
		<comp>
			<param name="top" value="WindowSill"/>
			<param name="side" value="WindowSill"/>
		</comp>
	</rule>
	<rule name="WindowSillTileLayer2_09">
		<extrude height="0.2"/>
		<comp>
			<param name="top" value="WindowSill"/>
			<param name="side" value="WindowSill"/>
		</comp>
	</rule>
	<rule name="WindowTileMain_09">
		<split splitAxis="x">
			<param type="relative" value="0.05" name="Wall"/>
			<param type="floating" value="1" name="WindowTileMainMain_09"/>
			<param type="relative" value="0.05" name="Wall"/>
		</split>
	</rule>
	<rule name="WindowTileMainMain_09">
		<extrude height="0.1"/>
		<comp>
			<param name="side" value="WindowFrame"/>
			<param name="top" value="WindowTileMainMainTop_09"/>
		</comp>
	</rule>
	<rule name="WindowTileMainMainTop_09">
		<offset offsetDistance="-offset_size" inside="WindowTileMainMainTopInside_09" border="WindowFrame"/>
	</rule>
	<rule name="WindowTileMainMainTopInside_09">
		<extrude height="-0.1"/>
		<comp>
			<param name="side" value="WindowFrame"/>
			<param name="top" value="WindowTileMainMainTopInsideBottom_09"/>
		</comp>
	</rule>
	<rule name="WindowTileMainMainTopInsideBottom_09">
		<offset offsetDistance="-0.02" inside="WindowTileMainMainTopInsideBottomInside_09" border="WindowFrame"/>
	</rule>
	<rule name="WindowTileMainMainTopInsideBottomInside_09">
		<extrude height="-0.05"/>
		<comp>
			<param name="side" value="WindowFrame"/>
			<param name="top" value="WindowGlassArea_09"/>
		</comp>
	</rule>
	<rule name="WindowGlassArea_09">
		<setupProjection axesSelector="scope.xy">
			<param name="texWidth" type="relative" value="1"/>
			<param name="texHeight" type="relative" value="1"/>
		</setupProjection>
		<split splitAxis="x">
			<param type="relative" value="0.3333" name="SeparatorAndWindowGlass_09" repeat="true"/>
		</split>
	</rule>
	<rule name="SeparatorAndWindowGlass_09">
		<split splitAxis="x">
			<param type="relative" value="0.02" name="WindowGlassSeparateTile_09"/>
			<param type="floating" value="1" name="WindowGlass"/>
			<param type="relative" value="0.02" name="WindowGlassSeparateTile_09"/>
		</split>
	</rule>
	<rule name="WindowGlassSeparateTile_09">
		<extrude height="0.05"/>
		<comp>
			<param name="side" value="WindowFrame"/>
			<param name="top" value="WindowFrame"/>
		</comp>
	</rule>
</grammar>
