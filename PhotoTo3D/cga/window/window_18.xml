<?xml version="1.0"?>
<grammar type="window" version="1.0" author="Gen Nishida">
	<attr name="window_depth" value="0.25" range="0.25,0.25"/>
	<rule name="Window">
		<extrude height="-window_depth"/>
		<comp>
			<param name="side" value="WindowFrame"/>
			<param name="top" value="WindowInsideBottom_31"/>
		</comp>
	</rule>
	<rule name="WindowInsideBottom_31">
		<offset offsetDistance="-0.02" inside="WindowInsideBottomInside_31" border="WindowFrame"/>
	</rule>
	<rule name="WindowInsideBottomInside_31">
		<extrude height="-0.05"/>
		<comp>
			<param name="side" value="WindowFrame"/>
			<param name="top" value="WindowGlassArea_31"/>
		</comp>
	</rule>
	<rule name="WindowGlassArea_31">
		<setupProjection axesSelector="scope.xy">
			<param name="texWidth" type="relative" value="1"/>
			<param name="texHeight" type="relative" value="1"/>
		</setupProjection>
		<split splitAxis="y">
			<param type="relative" value="0.7" name="WindowGlassCellArea_31"/>
			<param type="relative" value="0.3" name="WindowGlassTop_31"/>
		</split>
	</rule>
	<rule name="WindowGlassTop_31">
		<split splitAxis="x">
			<param type="relative" value="0.5" name="WindowGlassCellArea_31" repeat="true"/>
		</split>
	</rule>
	<rule name="WindowGlassCellArea_31">
		<split splitAxis="x">
			<param type="relative" value="0.025" name="WindowGlassSeparateTile_31"/>
			<param type="floating" value="1" name="WindowGlassCellColumnArea_31"/>
			<param type="relative" value="0.025" name="WindowGlassSeparateTile_31"/>
		</split>
	</rule>
	<rule name="WindowGlassCellColumnArea_31">
		<split splitAxis="y">
			<param type="relative" value="0.025" name="WindowGlassSeparateTile_31"/>
			<param type="floating" value="1" name="WindowGlass"/>
			<param type="relative" value="0.025" name="WindowGlassSeparateTile_31"/>
		</split>
	</rule>
	<rule name="WindowGlassSeparateTile_31">
		<extrude height="0.08"/>
		<comp>
			<param name="side" value="WindowFrame"/>
			<param name="top" value="WindowFrame"/>
		</comp>
	</rule>
</grammar>