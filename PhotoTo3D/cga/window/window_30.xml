<?xml version="1.0"?>
<grammar type="window" version="1.0" author="Gen Nishida">
    <attr value="0.05" name="window_side_offset" range="0.05,0.05"/>
    <attr value="0.05" name="window_top_offset" range="0.05,0.05"/>
    <rule name="Window">
        <split splitAxis="y">
            <param value="0.6" type="relative" name="DoorMain_21"/>
            <param value="0.1" type="relative" name="Wall"/>
            <param value="0.3" type="relative" name="DoorTop_21"/>
        </split>
    </rule>
    <rule name="DoorTop_21">
        <extrude height="-0.1"/>
        <comp>
            <param name="side" value="WindowFrame"/>
            <param name="top" value="WindowGlassArea_21"/>
        </comp>
    </rule>
    <rule name="WindowGlassArea_21">
        <setupProjection axesSelector="scope.xy">
            <param name="texWidth" type="relative" value="1"/>
            <param name="texHeight" type="relative" value="1"/>
        </setupProjection>
        <copy name="WindowGlass"/>
    </rule>
    <rule name="DoorMain_21">
        <extrude height="-0.2"/>
        <comp>
            <param value="Wall" name="side"/>
            <param value="EntranceTop_21" name="top"/>
        </comp>
    </rule>
    <rule name="EntranceTop_21">
        <split splitAxis="y">
            <param value="1-window_top_offset" type="relative" name="EntranceTopMain_21"/>
            <param value="window_top_offset" type="relative" name="Wall"/>
        </split>
    </rule>
    <rule name="EntranceTopMain_21">
        <split splitAxis="x">
            <param value="window_side_offset" type="relative" name="Wall"/>
            <param value="1-window_side_offset*2" type="relative" name="EntranceTopMainMain_21"/>
            <param value="window_side_offset" type="relative" name="Wall"/>
        </split>
    </rule>
    <rule name="EntranceTopMainMain_21">
        <extrude height="-0.6"/>
        <comp>
            <param value="Wall" name="side"/>
            <param value="DoorGlass" name="top"/>
        </comp>
    </rule>
</grammar>